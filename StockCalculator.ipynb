   "source": [
    "def EV(spread,pop,credit):\n",
    "    risk = spread - (credit)\n",
    "    riskEV = risk * (pop/100 - 1)\n",
    "    gainEV = credit * (pop/100)\n",
    "    expval = gainEV - riskEV\n",
    "    return expval"
   ]
